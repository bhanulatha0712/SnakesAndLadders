<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>calculator</servlet-name>
        <servlet-class>calculator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Createdata</servlet-name>
        <servlet-class>Createdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reg_insert</servlet-name>
        <servlet-class>reg_insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_Handler</servlet-name>
        <servlet-class>Login_Handler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newgame_details</servlet-name>
        <servlet-class>newgame_details</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>join_details</servlet-name>
        <servlet-class>join_details</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>board_controller</servlet-name>
        <servlet-class>board_controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>calculator</servlet-name>
        <url-pattern>/calculator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Createdata</servlet-name>
        <url-pattern>/Createdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reg_insert</servlet-name>
        <url-pattern>/reg_insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_Handler</servlet-name>
        <url-pattern>/Login_Handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newgame_details</servlet-name>
        <url-pattern>/newgame_details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>join_details</servlet-name>
        <url-pattern>/join_details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>board_controller</servlet-name>
        <url-pattern>/board_controller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
