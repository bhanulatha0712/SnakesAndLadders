body
{
	border:3px outset maroon;
}


#header
{
	height:100px;
	
	background-color:LightGray;
	border-bottom:3px;
	border-color:white;
	border-style:solid;
}

#menubar
{
	height:40px;
	
	background-color:black;
	border-bottom:3px;
	border-color:white;
	border-style:solid;
}

#menubar ul
{
	margin:auto;
}

#menubar li
{
	margin:3px 20px 3px 3px;
	list-style-type:none;
	display:inline;
}

#menubar li a
{
	color:white;
	text-decoration:none;
	font-weight:bold;
	font-size:29px;
	font-family:"MonoType Corsiva";
	padding:5px 12px 7px;
}

#menubar li a:hover
{
	color:NavajoWhite;
	text-decoration:underline;
	font-weight:bold;
}

#complete
{
	background-image:url(gray_back.jpg);
	height:850px;
}

#left 
{
	float:left;
	height:850px;
	width:200px;
}

#middle
{
	float:middle;
	height:850px;
	
}

#right
{
	float:right;
	height:850px;
	width:200px;
}

p
{
	font-size:25px;
	font-family:Harrington;
	color:maroon;
	text-indent:3px;
}

table
{
	font-family:"AR Cena";
	padding:5px;
	font-size:20px;

}

table tr
{
	height:20px;
}

table td
{
	height:40px;

}

a:hover
{
	text-decoration:none;
	color:maroon;
}